    {
	"type": "packages",
	"key": " "
    },
    {
	"type": "shell",
	"key": " ",
	"format": "{6}"
    },
    {
	"type": "terminal",
	"key": " ",
	"format": "{5}"
    },
    {
	"type": "memory",
	"key": "󰍛 "
    },
    "separator",
    {
	"type": "colors",
	"symbol": "circle" 
    }
  ]
}
